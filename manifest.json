{
	"manifest_version": 2,

	"name": "myTest",
	"description": "This extension will analyze a page using myTest",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Click here!"
	},
	"permissions": [
		// "tab",
		"activeTab",
		"<all_urls>"
	]
}